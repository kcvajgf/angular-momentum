SET statement_timeout = 0;
SET client_encoding = 'UTF8';
SET standard_conforming_strings = on;
SET check_function_bodies = false;
SET client_min_messages = warning;

--
-- Name: plpgsql; Type: EXTENSION; Schema: -; Owner: 
--

CREATE EXTENSION IF NOT EXISTS plpgsql WITH SCHEMA pg_catalog;


--
-- Name: EXTENSION plpgsql; Type: COMMENT; Schema: -; Owner: 
--

COMMENT ON EXTENSION plpgsql IS 'PL/pgSQL procedural language';


SET search_path = public, pg_catalog;

SET default_tablespace = '';

SET default_with_oids = false;

--
-- Name: problems; Type: TABLE; Schema: public; Owner: momentum; Tablespace: 
--

CREATE TABLE problems (
    id integer NOT NULL,
    index integer,
    title character varying(255),
    html character varying,
    answer character varying(255),
    release timestamp without time zone
);


ALTER TABLE public.problems OWNER TO momentum;

--
-- Name: problems_id_seq; Type: SEQUENCE; Schema: public; Owner: momentum
--

CREATE SEQUENCE problems_id_seq
    START WITH 1
    INCREMENT BY 1
    NO MINVALUE
    NO MAXVALUE
    CACHE 1;


ALTER TABLE public.problems_id_seq OWNER TO momentum;

--
-- Name: problems_id_seq; Type: SEQUENCE OWNED BY; Schema: public; Owner: momentum
--

ALTER SEQUENCE problems_id_seq OWNED BY problems.id;


--
-- Name: solved; Type: TABLE; Schema: public; Owner: momentum; Tablespace: 
--

CREATE TABLE solved (
    user_id integer NOT NULL,
    problem_id integer NOT NULL,
    created_at timestamp without time zone
);


ALTER TABLE public.solved OWNER TO momentum;

--
-- Name: users; Type: TABLE; Schema: public; Owner: momentum; Tablespace: 
--

CREATE TABLE users (
    id integer NOT NULL,
    username character varying(255),
    email character varying(255),
    password character varying,
    is_admin boolean
);


ALTER TABLE public.users OWNER TO momentum;

--
-- Name: users_id_seq; Type: SEQUENCE; Schema: public; Owner: momentum
--

CREATE SEQUENCE users_id_seq
    START WITH 1
    INCREMENT BY 1
    NO MINVALUE
    NO MAXVALUE
    CACHE 1;


ALTER TABLE public.users_id_seq OWNER TO momentum;

--
-- Name: users_id_seq; Type: SEQUENCE OWNED BY; Schema: public; Owner: momentum
--

ALTER SEQUENCE users_id_seq OWNED BY users.id;


--
-- Name: id; Type: DEFAULT; Schema: public; Owner: momentum
--

ALTER TABLE ONLY problems ALTER COLUMN id SET DEFAULT nextval('problems_id_seq'::regclass);


--
-- Name: id; Type: DEFAULT; Schema: public; Owner: momentum
--

ALTER TABLE ONLY users ALTER COLUMN id SET DEFAULT nextval('users_id_seq'::regclass);


