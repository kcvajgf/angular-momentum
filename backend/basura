SELECT pg_catalog.setval('users_id_seq', 2, true);


--
-- Name: problems_index_key; Type: CONSTRAINT; Schema: public; Owner: momentum; Tablespace: 
--

ALTER TABLE ONLY problems
    ADD CONSTRAINT problems_index_key UNIQUE (index);


--
-- Name: problems_pkey; Type: CONSTRAINT; Schema: public; Owner: momentum; Tablespace: 
--

ALTER TABLE ONLY problems
    ADD CONSTRAINT problems_pkey PRIMARY KEY (id);


--
-- Name: solved_pkey; Type: CONSTRAINT; Schema: public; Owner: momentum; Tablespace: 
--

ALTER TABLE ONLY solved
    ADD CONSTRAINT solved_pkey PRIMARY KEY (user_id, problem_id);


--
-- Name: users_pkey; Type: CONSTRAINT; Schema: public; Owner: momentum; Tablespace: 
--

ALTER TABLE ONLY users
    ADD CONSTRAINT users_pkey PRIMARY KEY (id);


--
-- Name: users_username_key; Type: CONSTRAINT; Schema: public; Owner: momentum; Tablespace: 
--

ALTER TABLE ONLY users
    ADD CONSTRAINT users_username_key UNIQUE (username);


--
-- Name: solved_problem_id_fkey; Type: FK CONSTRAINT; Schema: public; Owner: momentum
--

ALTER TABLE ONLY solved
    ADD CONSTRAINT solved_problem_id_fkey FOREIGN KEY (problem_id) REFERENCES problems(id);


--
-- Name: solved_user_id_fkey; Type: FK CONSTRAINT; Schema: public; Owner: momentum
--

ALTER TABLE ONLY solved
    ADD CONSTRAINT solved_user_id_fkey FOREIGN KEY (user_id) REFERENCES users(id);


--
-- Name: public; Type: ACL; Schema: -; Owner: postgres
--

REVOKE ALL ON SCHEMA public FROM PUBLIC;
REVOKE ALL ON SCHEMA public FROM postgres;
GRANT ALL ON SCHEMA public TO postgres;
GRANT ALL ON SCHEMA public TO PUBLIC;


