html, body {
  height: 100%;
  /* The html and body elements cannot have any padding or margin. */
}

/* Wrapper for page content to push down footer */
#sticky-wrap {
  min-height: 100%;
  height: auto !important;
  height: 100%;
  /* Negative indent footer by it's height */
  margin: 0 auto -40px;
}

/* Set the fixed height of the footer here */
#sticky-push,
footer {
  height: 40px;
}

footer.navbar {
  margin-bottom: 0;
}

/* Lastly, apply responsive CSS fixes as necessary */
@media (max-width: 767px) {
  footer {
    margin-left: -20px;
    margin-right: -20px;
    padding-left: 20px;
    padding-right: 20px;
  }
}

.problem-tab
  font-weight bold
  
.problem
  border 1px solid black
  margin 10px
  .index
    padding 2px
    .words
      font-weight bold
      font-size 20px
    background-color #eee
  .title
    padding 2px
    font-weight bold
    font-size 15px
    background-color #eee
  .body
